{
    "name": "laminas-api-tools/api-tools-admin",
    "description": "Laminas API Tools Admin module",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "framework"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-admin/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-admin",
        "rss": "https://github.com/laminas-api-tools/api-tools-admin/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.3.3",
        "laminas-api-tools/api-tools": "dev-master",
        "laminas-api-tools/api-tools-api-problem": "dev-master",
        "laminas-api-tools/api-tools-configuration": "dev-master",
        "laminas/laminas": "dev-develop",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "rwoverdijk/assetmanager": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Laminas\\ApiTools\\Admin\\": "src/",
            "LaminasTest\\ApiTools\\": "test/",
            "Test\\": "vendor/"
        }
    },
    "replace": {
        "zfcampus/zf-apigility-admin": "self.version"
    }
}
